{
  "name": "journal",
  "version": "0.1.0",
  "private": true,
  "homepage": "htts://yurenju.github.io/journal",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/react-map-gl": "^3.3.3",
